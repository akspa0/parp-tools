<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>PM4NextExporter</RootNamespace>
    <AssemblyName>PM4NextExporter</AssemblyName>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <!-- Reference shared parpToolbox utilities (ProjectOutput, etc.) -->
    <ProjectReference Include="..\parpToolbox\parpToolbox.csproj" />
  </ItemGroup>
  <ItemGroup>
    <!-- Exclude deprecated/disabled instance partitioner from compilation -->
    <Compile Remove="Assembly\CompositeHierarchyInstanceAssembler.cs" />
  </ItemGroup>
</Project>
